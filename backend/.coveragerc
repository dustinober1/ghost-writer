[run]
source = app
omit = 
    */tests/*
    */test_*.py
    */__pycache__/*
    */alembic/*
    */venv/*
    */env/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

precision = 2

[html]
directory = htmlcov